{
    "name": "Outline Redirect",
    "description": "Creates a tinyurl of the current page which is then opened in outline.com",
    "version": "1.0",
    "manifest_version": 2,

    "permissions": [
    "tabs",
    "http://*/*"
    ],

  "icons": {
    "16": "outline.16.png",
    "48": "outline.48.png",
    "96": "outline.96.png",
    "192": "outline.192.png"
  },
    
    "background": {
        "scripts": ["redirect.js"]
    },
    "browser_action": {}
}
